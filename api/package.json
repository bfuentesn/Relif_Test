{
  "name": "automotora-api",
  "version": "1.0.0",
  "description": "API backend para sistema CRM de automotora",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "prisma db seed",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^13.0.0",
    "@prisma/client": "^5.8.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-json": "^2.0.2",
    "openai": "^4.24.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/koa": "^2.13.12",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-json": "^2.0.21",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.10.6",
    "prisma": "^5.8.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
